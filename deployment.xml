<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<package xmlns="http://www.zend.com/server/deployment-descriptor/1.0" version="1.0">
    <name>Symfony2Cloud</name>
    <version>
        <release>1.0.0</release>
    </version>
    <appdir>data</appdir>
    <docroot>data/web</docroot>
    <scriptsdir>scripts</scriptsdir>
    <parameters>
    <parameter display="Container" id="container" readonly="false" required="true" type="string">
      <description>Zend Developer Cloud conatiner name that this applications is deployed to</description>
    </parameter>
    <parameter display="Password" id="password" readonly="false" required="true" type="password">
      <description>Zend Developer Cloud conatiner's password </description>
    </parameter>
  </parameters>
</package>
